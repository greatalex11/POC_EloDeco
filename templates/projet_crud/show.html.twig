{% extends 'base.html.twig' %}

{% block title %}Projet{% endblock %}

{% block body %}
    <h1>Projet</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ projet.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ projet.titre }}</td>
            </tr>
            <tr>
                <th>Debut</th>
                <td>{{ projet.Debut ? projet.Debut|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>DateFin</th>
                <td>{{ projet.dateFin ? projet.dateFin|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ projet.description }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_projet_crud_index') }}">back to list</a>

    <a href="{{ path('app_projet_crud_edit', {'id': projet.id}) }}">edit</a>

    {{ include('projet_crud/_delete_form.html.twig') }}
{% endblock %}
